	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug TS File",
			"type": "node",
			"request": "launch",
			"program": "${file}",
			"runtimeExecutable": "${workspaceRoot}/node_modules/.bin/tsx",
			"skipFiles": [
				"<node_internals>/**",
				"${workspaceFolder}/node_modules/**",
			]
		}, {
			"name": "Debug Test",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/src/test.ts",
			"runtimeExecutable": "${workspaceRoot}/node_modules/.bin/tsx",
			"skipFiles": [
				"<node_internals>/**",
				"${workspaceFolder}/node_modules/**",
			]
		},
	]
}
